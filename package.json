{
  "name": "messenger",
  "version": "1.0.0",
  "description": "custom messenger like telegram, whatsapp",
  "main": "src/index.js",
  "source": "src/index.html",
  "targets": {
    "main": false,
    "default": {
      "sourceMap": false,
      "engines": {
        "browsers": ">=50%"
      }
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "dev": "parcel src/index.html --no-cache --open",
    "start": "parcel build  && node server.ts --open",
    "prebuild": "rm -rf dist",
    "eslint": "eslint .  --fix",
    "build": "parcel build",
    "lint": "npx stylelint \"**/*.scss\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vakulina/middle.messenger.praktikum.yandex.git"
  },
  "keywords": [
    "messenger",
    "typescript"
  ],
  "author": "V.Yakovleva",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Vakulina/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/Vakulina/middle.messenger.praktikum.yandex#readme",
  "dependencies": {
    "express": "4.18.2",
    "handlebars": "4.7.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.8.3",
    "@parcel/transformer-sass": "2.8.3",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "eslint": "8.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "parcel": "2.8.3",
    "parcel-transformer-hbs": "1.0.4",
    "process": "0.11.10",
    "sass": "1.57.1",
    "stylelint": "14.16.1",
    "stylelint-config-standard-scss": "6.1.0",
    "ts-node": "^10.9.1",
    "typescript": "4.9.4"
  }
}
